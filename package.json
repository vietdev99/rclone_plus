{
  "name": "rclone-plus",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "description": "Server-to-Server Data Transfer Application using Rclone and Google Drive",
  "author": "RClone Plus Team",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && npm run build:main && electron .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "rclone",
    "ssh",
    "file-transfer",
    "google-drive"
  ],
  "dependencies": {
    "@tanstack/react-query": "^5.90.16",
    "electron-store": "^8.2.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ssh2": "^1.17.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/ssh2": "^1.15.5",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^32.3.3",
    "electron-builder": "^26.4.0",
    "png-to-ico": "^3.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  }
}
